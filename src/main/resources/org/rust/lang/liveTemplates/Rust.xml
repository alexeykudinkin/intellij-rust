<?xml version='1.0' encoding='UTF-8'?>

<templateSet group="Rust">
    <template name="p" description="print" toReformat="true" toShortenFQNames="true"
              value="println!(&quot;$END$&quot;);">

        <context>
            <option name="RUST_FILE" value="true"/>
        </context>
    </template>

    <template name="pd" description="debug print" toReformat="true" toShortenFQNames="true"
              value="println!(&quot;{:?}&quot;, $END$);">

        <context>
            <option name="RUST_FILE" value="true"/>
        </context>
    </template>

    <template name="ppd" description="debug pretty print" toReformat="true" toShortenFQNames="true"
              value="println!(&quot;{:#?}&quot;, $END$);">

        <context>
            <option name="RUST_FILE" value="true"/>
        </context>
    </template>

    <template name="fn0" description="Function with no parameters" toReformat="true" toShortenFQNames="true"
              value="$PUB$fn $NAME$() -&gt; $RETURN$ {&#10;  $END$&#10;}">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="PUB" expression="" defaultValue="&quot;pub &quot;" alwaysStopAt="true" />
        <variable name="RETURN" expression="" defaultValue="&quot;()&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="fn1" description="Function with one parameter" toReformat="true" toShortenFQNames="true"
              value="$PUB$fn $NAME$($PARAM1$: $PARAM1TYPE$) -&gt; $RETURN$ {&#10;  $END$&#10;}">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="PUB" expression="" defaultValue="&quot;pub &quot;" alwaysStopAt="true" />
        <variable name="PARAM1" expression="" defaultValue="&quot;x&quot;" alwaysStopAt="true" />
        <variable name="PARAM1TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <variable name="RETURN" expression="" defaultValue="&quot;()&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="fn2" description="Function with two parameters" toReformat="true" toShortenFQNames="true"
              value="$PUB$fn $NAME$($PARAM1$: $PARAM1TYPE$, $PARAM2$: $PARAM2TYPE$) -&gt; $RETURN$ {&#10;  $END$&#10;}">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="PUB" expression="" defaultValue="&quot;pub &quot;" alwaysStopAt="true" />
        <variable name="PARAM1" expression="" defaultValue="&quot;x&quot;" alwaysStopAt="true" />
        <variable name="PARAM1TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <variable name="PARAM2" expression="" defaultValue="&quot;y&quot;" alwaysStopAt="true" />
        <variable name="PARAM2TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <variable name="RETURN" expression="" defaultValue="&quot;()&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="loop" description="Infinite loop" toReformat="true" toShortenFQNames="true"
              value="loop {&#10;  $SELECTION$&#10;}$END$">
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="while" description="Conditional loop" toReformat="true" toShortenFQNames="true"
              value="while $CONDITION$ {&#10;  $SELECTION$&#10;}$END$">
        <variable name="CONDITION" expression="" defaultValue="&quot;true&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="for" description="Iterator loop" toReformat="true" toShortenFQNames="true"
            value="for $VARIABLE$ in $EXPRESSION$ {&#10;  $SELECTION$&#10;}$END$">
        <variable name="VARIABLE" expression="" defaultValue="&quot;x&quot;" alwaysStopAt="true" />
        <variable name="EXPRESSION" expression="" defaultValue="&quot;0..10&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="struct0" description="Unit-like struct" toReformat="true" toShortenFQNames="true"
            value="struct $NAME$;$END$">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="struct1" description="Struct with one field" toReformat="true" toShortenFQNames="true"
            value="struct $NAME$ {&#10;  $FIELD1$: $FIELD1TYPE$,&#10;}$END$">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="FIELD1" expression="" defaultValue="&quot;x&quot;" alwaysStopAt="true" />
        <variable name="FIELD1TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="struct2" description="Struct with two fields" toReformat="true" toShortenFQNames="true"
            value="struct $NAME$ {&#10;  $FIELD1$: $FIELD1TYPE$,&#10;  $FIELD2$: $FIELD2TYPE$,&#10;}$END$">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="FIELD1" expression="" defaultValue="&quot;x&quot;" alwaysStopAt="true" />
        <variable name="FIELD1TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <variable name="FIELD2" expression="" defaultValue="&quot;y&quot;" alwaysStopAt="true" />
        <variable name="FIELD2TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="tstruct1" description="Tuple struct with one field" toReformat="true" toShortenFQNames="true"
            value="struct $NAME$($FIELD1TYPE$);$END$">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="FIELD1TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="tstruct2" description="Tuple struct with two fields" toReformat="true" toShortenFQNames="true"
            value="struct $NAME$($FIELD1TYPE$, $FIELD2TYPE$);$END$">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="FIELD1TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <variable name="FIELD2TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

    <template name="enum" description="Enum" toReformat="true" toShortenFQNames="true"
              value="enum $NAME$ {&#10;  $END$&#10;}">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="RUST_FILE" value="true" />
        </context>
    </template>

</templateSet>
